{
  "name": "escreg-sdk",
  "version": "0.0.1",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rm -rf dist/ src/generated/",
    "prebuild": "npm run clean && npm run generate",
    "generate": "npx @algorandfoundation/algokit-client-generator generate -o src/generated/EscregGenerated.ts -a ../contract/smart_contracts/artifacts/escreg/Escreg.arc56.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build": "concurrently 'npm run build:cjs' 'npm run build:esm'"
  },
  "author": "Tasos Bitsios",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@algorandfoundation/algokit-client-generator": "^6.0.0",
    "@types/node": "^24.0.14",
    "concurrently": "^9.2.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@algorandfoundation/algokit-utils": "^9.0.0",
    "algosdk": "^3.0.0"
  },
  "dependencies": {
    "p-map": "^7.0.3"
  }
}
